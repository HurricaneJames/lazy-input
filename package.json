{
  "name": "lazy-input",
  "version": "2.0.0",
  "description": "A lazy React.js input field that only updates when it is told to re-render (fixes issues with Flux backed field data)",
  "main": "dist/LazyInput.js",
  "scripts": {
    "build": "./scripts/build",
    "test": "./node_modules/.bin/mocha --require ./babel.js --compilers jsx:babel/register --recursive ./src/__tests__",
    "test-cov": "./scripts/test-cov",
    "prepublish": "npm run build",
    "examples": "webpack-dev-server --progress --colors --port 8090"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HurricaneJames/lazy-input"
  },
  "keywords": [
    "react",
    "react-component",
    "flux",
    "input",
    "textarea",
    "lazy",
    "optimistic",
    "render"
  ],
  "author": "James Burnett <HurricaneJamesEsq@gmail.com> (https://github.com/HurricaneJames/lazy-input)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HurricaneJames/lazy-input/issues"
  },
  "homepage": "https://github.com/HurricaneJames/lazy-input",
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.11",
    "babel-loader": "^5.1.3",
    "babel-runtime": "^5.5.4",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "expect.js": "^0.3.1",
    "istanbul": "~0.3.7",
    "jsdom": "^3.1.0",
    "mocha": "^2.0.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.2.7",
    "sinon": "^1.14.1",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  }
}
